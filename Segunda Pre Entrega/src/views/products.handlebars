<h1>Productos</h1>
<table id="main">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Code</th>
    <th>Price</th>
    <th>Status</th>
    <th>Stock</th>
    <th>Category</th>
    <th>Thumbnail</th>
  </tr>
  {{#each payload}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.code}}</td>
      <td>{{this.price}}</td>
      <td>{{this.status}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.category}}</td>
      <td>{{this.thumbnail}}</td>
    </tr>
  {{/each}}
</table>

<p>Pagina actual: {{page}}</p>
<a href="{{prevPage}}" role="button" {{#unless hasPrevPage}} disabled {{/unless}}>Anterior</a>
<a href="{{nextPage}}" role="button" {{#unless hasNextPage}} disabled {{/unless}}>Siguiente</a>