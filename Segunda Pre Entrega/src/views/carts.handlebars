<h1>Carts</h1>
<table id="main">
  <tr>
    <th><b>Cart Id</b></th>
    <th><b>Product Id</b></th>
    <th><b>Quantity</b></th>
  </tr>
  <tr>
    <td>{{cartId}}</td>
    <td></td>
    <td></td>
  </tr>
  {{#if products}}
    {{#each products}}
      <tr>
        <td></td>
        <td>{{this.pid}}</td>
        <td>{{this.quantity}}</td>
      </tr>
    {{/each}}
  {{/if}}
</table>